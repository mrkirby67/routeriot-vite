{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/ff/player",
        "destination": "/ff/ff_player.html"
      },
      {
        "source": "/ff/control",
        "destination": "/ff/ff_control.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
